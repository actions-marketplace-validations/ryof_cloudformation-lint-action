{
  "problemMatcher": [
    {
      "owner": "cfn-lint",
      "pattern": [
        {
          "regexp": "^(\\S+):(\\d+):(\\d+):\\d+:\\d+:(error|warning|debug)(\\d{4}):(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}